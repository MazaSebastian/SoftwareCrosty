{
  "version": 2,
  "name": "crosty-software",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "REACT_APP_VERSION": "1.0.0",
    "REACT_APP_NAME": "CROSTY Software",
    "REACT_APP_ENVIRONMENT": "production"
  }
}
